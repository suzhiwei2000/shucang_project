page {
  background-color: #F2F2F2;
}
.real-fail {
  padding: 30rpx 20rpx;
}
.real-fail .real-item {
  display: flex;
  align-items: center;
  color: #000;
  font-size: 30rpx;
  background: #FFF;
  border-radius: 20rpx;
  padding: 20rpx 30rpx;
  margin-bottom: 50rpx;
}
.real-fail .real-item .real-title {
  width: 27%;
  text-align: center;
}
.real-fail .real-item .real-name {
  letter-spacing: 10rpx;
}
.real-fail .real-item .real-input {
  margin-left: 20rpx;
}
.real-fail .real-submit {
  width: 80%;
  padding: 20rpx 50rpx;
  text-align: center;
  margin: 100rpx auto 0;
  background: #FFF;
  color: #000;
  border-radius: 50rpx;
  font-size: 36rpx;
  font-weight: 700;
}
.real-suc {
  padding: 120rpx 50rpx;
  text-align: center;
  color: #000;
  font-size: 30rpx;
}
.real-suc .real-suc-title {
  font-size: 50rpx;
  margin: 40rpx 0 80rpx;
}
.real-suc .real-suc-id {
  width: 100%;
  position: relative;
  color: #000;
}
.real-suc .real-suc-id .real-suc-id-img {
  width: 100%;
}
.real-suc .real-suc-id .real-suc-id-name {
  position: absolute;
  top: 100rpx;
  left: 50rpx;
}
.real-suc .real-suc-id .real-suc-id-idNum {
  position: absolute;
  top: 250rpx;
  left: 50rpx;
}

