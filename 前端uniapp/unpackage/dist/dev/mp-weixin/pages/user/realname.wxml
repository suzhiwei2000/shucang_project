<view class="realname"><block wx:if="{{!realType}}"><view class="real-fail"><view class="real-item"><view class="real-title real-name">姓名 :</view><input class="real-input" focus="{{true}}" type="text" placeholder="请输入姓名" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="real-item"><view class="real-title">身份证号码 :</view><input class="real-input" type="text" placeholder="请输入身份证号码" data-event-opts="{{[['input',[['__set_model',['','id','$event',[]]]]]]}}" value="{{id}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="real-submit" bindtap="__e">立即提交</view></view></block><block wx:if="{{realType}}"><view class="real-suc"><icon class="icon-a" type="success" size="70" color="#9CE480"></icon><view class="real-suc-title">已实名认证</view><view class="real-suc-id"><image class="real-suc-id-img" src="../../static/userinfo.png" mode="widthFix"></image><view class="real-suc-id-name">{{realName}}</view><view class="real-suc-id-idNum">{{realID}}</view></view></view></block></view>