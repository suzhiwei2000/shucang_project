page {
  background: #fff !important;
}
.contain {
  padding: 26rpx 20rpx 0;
}
.contain .list {
  position: relative;
  margin-bottom: 40rpx;
  height: 320rpx;
  box-shadow: 0 0 8rpx 0 #bbb;
  border-radius: 10rpx;
  box-sizing: border-box;
}
.contain .list .active-status {
  position: absolute;
  left: -6px;
  top: 30px;
  width: 66px;
  height: 30px;
  line-height: 29px;
  text-align: center;
  font-size: 11px;
  background: #d7b059;
  background-size: 100% auto;
  border-top-right-radius: 13px;
  border-bottom-right-radius: 13px;
  opacity: .8;
  color: #fff;
}
.contain .list .img {
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  left: 32%;
  bottom: 32%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  margin: auto;
}
.contain .list .assets_info {
  width: 296rpx;
  padding-top: 40rpx;
  position: absolute;
  top: 0;
  right: 52rpx;
  z-index: 3;
}
.contain .list .assets_info .assets_name {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  color: #000;
}
.contain .list .assets_info .btn_open {
  width: 140rpx;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  border-radius: 6rpx;
  box-shadow: 0 0 8rpx #ddd inset;
  margin-top: 60rpx;
  font-size: 14px;
  color: #555;
}
.contain .list .booth-top {
  width: 320rpx;
  height: 118rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-position: 50%;
  position: absolute;
  bottom: -5px;
  left: 33px;
  z-index: 2;
}
.contain .list .booth-bottom {
  width: 620rpx;
  height: 80rpx;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-position: 50%;
  position: absolute;
  bottom: 5px;
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
  z-index: 1;
}

