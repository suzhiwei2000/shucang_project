<view class="data-v-1b0703f2"><view class="box_top data-v-1b0703f2"><view class="search data-v-1b0703f2"><input type="text" value="{{search}}" placeholder="请输入商品名称" data-event-opts="{{[['input',[['__set_model',['','search','$event',[]]]]]]}}" bindinput="__e" class="data-v-1b0703f2"/></view><view data-event-opts="{{[['tap',[['sub',['$event']]]]]}}" class="sub data-v-1b0703f2" bindtap="__e">确定</view></view><view class="select_box data-v-1b0703f2"><block wx:for="{{selectArr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="select_item data-v-1b0703f2" bindtap="__e"><view class="{{['name','data-v-1b0703f2',item.show?'select_active':'']}}">{{item.name}}</view><view class="icon data-v-1b0703f2"><view class="util data-v-1b0703f2"><image src="{{item.show&&item.tip?item.icon[0].change:item.icon[0].noChange}}" mode class="data-v-1b0703f2"></image></view><view class="util data-v-1b0703f2"><image src="{{item.show&&!item.tip?item.icon[1].change:item.icon[1].noChange}}" mode class="data-v-1b0703f2"></image></view></view></view></block></view><view class="contain data-v-1b0703f2"><block wx:for="{{arr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['link',['$0','$1'],[[['arr','',index,'id']],[['arr','',index,'goods_id']]]]]]]}}" class="item data-v-1b0703f2" bindtap="__e"><view class="img data-v-1b0703f2"><image src="{{item.file_path}}" mode class="data-v-1b0703f2"></image></view><view class="last data-v-1b0703f2"><view class="name data-v-1b0703f2">{{item.name}}</view><view class="number data-v-1b0703f2"><view class="numText data-v-1b0703f2"><view class="title data-v-1b0703f2">编号</view>{{item.coll_no+''}}</view><view class="price data-v-1b0703f2">{{"￥"+item.price}}</view></view></view></view></block></view></view>